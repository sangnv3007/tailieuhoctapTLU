else if(p[0]=='AVIM_method')method=p[1]
else if(p[0]=='AVIM_ckspell'){if(p[1]==0){dockspell=0;spellerr=nospell}
else{dockspell=1;spellerr=ckspell}}else if(p[0]=='AVIM_daucu')dauCu=parseInt(p[1])}}
