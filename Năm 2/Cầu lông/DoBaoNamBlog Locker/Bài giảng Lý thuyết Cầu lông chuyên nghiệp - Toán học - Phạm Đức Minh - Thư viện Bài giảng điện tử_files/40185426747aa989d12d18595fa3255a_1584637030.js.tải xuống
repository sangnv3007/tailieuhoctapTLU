if(operaVersion>=8)is_opera=true}}else if(agt.indexOf("khtml")>=0)isKHTML=true
else{ver=agt.substr(agt.indexOf("rv:")+3)
ver=parseFloat(ver.substr(0,ver.indexOf(" ")))
if(agt.indexOf("mozilla")<0)ver=0}}
