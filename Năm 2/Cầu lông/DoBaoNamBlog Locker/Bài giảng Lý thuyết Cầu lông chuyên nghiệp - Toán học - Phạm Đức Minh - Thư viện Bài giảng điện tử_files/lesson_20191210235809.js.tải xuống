var acronym_subject_data = {
  "toan hoc":"toan", "so hoc": "sh", "dai so":"dai,ds", "giai tich":"gt", "dai so va giai tich":"dsgt,ds&gt,ds gt,dai so giai tich", "hinh hoc":"hinh,hh", "ngu van":"van,nv", "tieng anh":"anh,ta,english",
  "vat li":"li,ly,vat ly,vl", "hoa hoc":"hoa", "sinh hoc":"sinh", "lich su":"su,ls", "dia li":"dia,dia ly,dl",
  "am nhac":"nhac,an", "mi thuat":"my thuat,mt", "the duc":"", "cong nghe":"cn", "tin hoc":"tin,th", "giao duc cong dan": "gdcd,gd cong dan",
