  "tu nhien va xa hoi":"tnxh,tn&xh,tnvxh,tn xh,tu nhien xa hoi", "khoa hoc":"kh", "ki thuat":"ky thuat,kt", "thu cong":"tc", "dao duc":"dd", "an toan giao thong":"atgt", "cung hoc tin hoc":"chth", "huong dan hoc tin hoc":"hdth,hdhth,huong dan tin hoc",
  "hoat dong ngoai gio len lop":"hoat dong ngll,hdngll,ngll","giao duc quoc phong an ninh":"quoc phong,gdqpan,gdqp,qpan", "giao duc huong nghiep":"huong nghiep,gdhn",
  "cat may":"", "sua chua xe dap":"", "nau an":"", "trong cay an qua":"", "lap dat mang dien trong nha":"",
  "let s learn":"", "let s go":"", "family and friends":"", "first friends":"", "abc english":"",
  "giao an theo tuan lop": "van hoa",
